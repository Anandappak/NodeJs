1)Object to list
let obj = {
  key1: 'value1',
  key2: 'value2',
  key3: 'value3'
};

let customArray = Object.keys(obj).map(key => {
  return { key: key, value: obj[key] };
});

console.log(customArray); 
// Output: [{key: 'key1', value: 'value1'}, {key: 'key2', value: 'value2'}, {key: 'key3', value: 'value3'}]
